{
    "theme": {
        "background": "#0E0E0E",
        "surface": "#1A1A1A",
        "surfaceAlt": "#222222",
        "divider": "#2A2A2A",
        "textPrimary": "#FFFFFF",
        "textSecondary": "#B3B3B3",
        "textMuted": "#6B6B6B",
        "accent": "#FC4C02"
    },
    "components": {
        "OnboardingSlide": {
            "props": [
                "image",
                "title",
                "subtitle"
            ]
        },
        "OnboardingCarousel": {
            "props": [
                "slides",
                "autoAdvanceMs"
            ]
        },
        "AuthForm": {
            "variants": [
                "login",
                "signup"
            ],
            "props": [
                "email",
                "password",
                "name?"
            ]
        },
        "SocialButton": {
            "props": [
                "provider",
                "label"
            ]
        },
        "WorkoutCard": {
            "props": [
                "title",
                "subtitle",
                "duration",
                "exercises",
                "progress"
            ],
            "states": [
                "notStarted",
                "inProgress",
                "completed"
            ]
        },
        "QuickStartCard": {
            "props": [
                "title",
                "icon"
            ]
        },
        "RecentItem": {
            "props": [
                "title",
                "date",
                "duration"
            ]
        },
        "BottomNav": {
            "props": [
                "selectedIndex"
            ],
            "tabs": [
                "Home",
                "Workouts",
                "AI",
                "Profile"
            ]
        },
        "ProfileHeader": {
            "props": [
                "name",
                "memberSince",
                "avatarUrl"
            ]
        },
        "StatCard": {
            "props": [
                "value",
                "label"
            ]
        }
    },
    "screens": {
        "Auth": [
            "OnboardingCarousel",
            "AuthForm",
            "SocialButtons"
        ],
        "Home": [
            "Header",
            "WorkoutCard",
            "QuickStart",
            "RecentList",
            "BottomNav"
        ],
        "Workouts": [
            "CategoryList",
            "BottomNav"
        ],
        "AI": [
            "EmptyState",
            "BottomNav"
        ],
        "Profile": [
            "ProfileHeader",
            "Stats",
            "Settings",
            "BottomNav"
        ]
    }
}